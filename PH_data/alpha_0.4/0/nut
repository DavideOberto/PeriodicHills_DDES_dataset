FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 6.7e-06;

boundaryField
{
    hill
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
    top-wall
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    cyclicAMI_outlet
    {
        type            cyclicAMI;
        value           uniform 6.7e-06;
    }
    cyclicAMI_inlet
    {
        type            cyclicAMI;
        value           uniform 6.7e-06;
    }
}


